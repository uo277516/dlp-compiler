

def f(a :: boolean, b :: boolean, c :: boolean) :: boolean do
    return (a && b) || !c
end

def main() do
   b1, b2, b3 :: boolean
   a :: int

   puts f(true, false, false)
   puts f(true, false, true)
   puts f(false, true, true)
   puts f(false, false, false)

   b1 = true
   b2 = 3 > 1
   b3 = 4.5 <= 4.2
   puts b1
   puts b2
   puts b3

   if b1 do
        puts 'Y'
   else
        puts 'N'
   end

   a = 0

   while a < 10 do
        puts a
        a = a+1
   end

end